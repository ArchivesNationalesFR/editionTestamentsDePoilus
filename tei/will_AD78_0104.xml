    <text xml:id="will_AD78_0104">
        <body>
            <div type="will">
                <pb facs="#FRAD78_Poilus_t-0104_02"/>
                <head>Ceci est mon testament : </head>
                <p>Je donne tout ce que <lb/>je
                        <choice><sic>possede</sic><corr>possède</corr></choice> à ma soeur
                        <lb/><persName>Marie Cros</persName>.</p>
                <dateline rend="right"><placeName type="willPlace" ref="#pl-600">Gespunsart</placeName> <date type="willDate" when="1914-08-24">24
                                <choice><sic>aout</sic><corr>août</corr></choice>
                            1914</date><choice><sic/><corr>.</corr></choice></dateline>
                <signed rend="centered"><persName ref="#pers-389">G.
                        <surname>Cros</surname></persName></signed> 
                    <postscript><p><choice><abbr>L<hi rend="superscript">t</hi> Colonel</abbr><expan>Lieutenant-colonel</expan></choice> au
                                <orgName><unclear>5<hi rend="superscript">e</hi></unclear> <choice><abbr>Tirailleur</abbr><expan>régiment de
                                tirailleurs algériens</expan></choice></orgName><choice><sic/><corr>.</corr></choice></p></postscript>
                <metamark function="other" rend="horizontal-line"/>
            </div>
            <div type="envelope">
                <pb facs="#FRAD78_Poilus_t-0104_01"/>
                <p rend="centered"><app><lem><hi rend="underlined">Testament</hi></lem><note resp="#PCharbonnier">La page est pliée et fait fonction d’enveloppe.</note></app></p>
                <space dim="vertical" unit="lines" quantity="11"/>
                <p rend="centered"><hi rend="underlined">Testament</hi></p>
            </div>
        </body>
    </text>
