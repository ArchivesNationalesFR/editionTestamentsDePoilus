    <text xml:id="will_AN_0185">
        <body>
            <div type="will">
                <pb facs="#FRAN_Poilus_t-0185_01"/>
                <dateline rend="right"><placeName type="willPlace" ref="#pl-2">Paris</placeName>, <date
                        type="willDate" when="1914-08-05">5
                            <choice><sic>Aout</sic><corr>août</corr></choice> 1914</date></dateline>
                <space dim="vertical" unit="lines" quantity="5"/>
                <p>Je soussigné <persName ref="#pers-247">Frédéric
                            Jacques <lb/><surname>Arbouin</surname></persName>,
                            <choice><abbr>s/Lieutenant</abbr><expan>sous-lieutenant</expan></choice>
                    au <orgName ref="#o-202">31<hi rend="superscript-underlined"
                            >e</hi></orgName> <lb/><choice><abbr>Régiment d’Inf<hi
                                rend="superscript-underlined">ie</hi></abbr><expan>régiment
                            d’infanterie</expan></choice>, déclare léguer <lb/>par le présent acte
                    tous mes <lb/>biens présents
                        <choice><abbr>&amp;</abbr><expan>et</expan></choice> à venir
                        à <lb/><persName>Mademoiselle Jacqueline
                    <surname>Eck</surname></persName>, <lb/>née <date type="otherDate"
                        when="1892-07-07">le 7 juillet 1892</date>.</p>
                <dateline>Fait à <placeName type="willPlace" ref="#pl-2">Paris</placeName>, <date
                        type="willDate" when="1914-08-05">le cinq
                                <choice><sic>aout</sic><corr>août</corr></choice> <lb/>mil neuf cent
                        quatorze</date>, sur papier libre.</dateline>
                <signed rend="right"><persName ref="#pers-247">Frédéric J.
                            <surname>Arbouin</surname></persName></signed>
            </div>
            <div type="envelope">
                <pb facs="#FRAN_Poilus_t-0185_02"/>
                <p><address>
                        <addrLine><persName
                                ref="#pers-247"><choice><abbr>M</abbr><expan>Monsieur</expan></choice> <surname>Arbouin</surname> (Jacques
                                        <choice><sic>Frederic</sic><corr>Frédéric</corr></choice>)</persName></addrLine>
                        <addrLine>demeurant <choice><sic>a</sic><corr>à</corr></choice> <placeName
                                type="willAuthorResidence" ref="#pl-396">Poissy</placeName>
                                    (<choice><sic>Seine &amp;
                                Oise</sic><corr>Seine-et-Oise</corr></choice>)</addrLine>
                        <addrLine>avenue Meisssonnier n<hi rend="superscript">o</hi> 28</addrLine>
                    </address></p>
                <pb facs="#FRAN_Poilus_t-0185_03"/>
                <p rend="centered"><choice><sic>A</sic><corr>À</corr></choice> n’ouvrir qu’en cas de décès<choice><sic></sic><corr>.</corr></choice></p>
                <metamark function="emphasis" rend="horizontal-line"/>
            </div>
        </body>
    </text>
