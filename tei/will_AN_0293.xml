    <text xml:id="will_AN_0293">
        <body>
            <div type="will">
                <pb facs="#FRAN_Poilus_t-0293_01"/>
                <p>Je soussigné <persName
                        ref="#pers-346">Georges <lb/>Marcel <surname>Arnould</surname></persName> né
                    <date type="willAuthorBirthDate" when="1887-09-30">le 30 <lb/>septembre
                        1887</date> lègue à ma <lb/>femme, née <persName>Louise Alexina
                        <lb/>Jacquin</persName>, tous mes biens, <lb/>meubles, linge, bijoux,
                    valeurs, <lb/>espèces, m’appartenant ; et en <lb/>fais ma légataire
                            universelle<choice><sic/><corr>.</corr></choice></p>
                <dateline>Fait à <placeName type="willPlace" ref="#pl-2">Paris</placeName> <date
                        type="willDate" when="1914-08-05">le 5
                            <choice><sic>Août</sic><corr>août</corr></choice>
                                1914<choice><sic/><corr>.</corr></choice></date></dateline>
                <signed rend="centered"><persName ref="#pers-346">M
                        <surname>Arnould</surname></persName></signed> <postscript rend="centered"><p><hi rend="underlined">194, rue de
                        Rivoli</hi></p></postscript>
            </div>
        </body>
    </text>
