    <text xml:id="will_AN_0214">
        <body>
            <div type="will">
                <pb facs="#FRAN_Poilus_t-0214_01"/>
                <head rend="centered">Ceci est mon testament</head>
                <metamark function="separating" rend="horizontal-line"/>
                <space dim="vertical" unit="lines" quantity="1"/>
                <p>J’institue ma légataire <lb/>universelle mon
                            <choice><sic>epouse</sic><corr>épouse</corr></choice> <lb/><persName>Madame
                        S. Vincent-Darasse <lb/>(née Suzanne Chabanian)</persName> <lb/>qui disposera
                    de mes <lb/>biens à sa volonté.</p>
                <space dim="vertical" unit="lines" quantity="1"/>
                <dateline rend="centered">Aux Armées. <lb/><space dim="horizontal" unit="chars"
                        quantity="3"/><date
                            type="willDate" when="1915-09-12"><choice><sic>le</sic><corr>Le</corr></choice> 12
                            <choice><sic>Septembre</sic><corr>septembre</corr></choice> 1915</date><choice><sic></sic><corr>.</corr></choice></dateline>
                <signed rend="centered"><persName ref="#pers-276"
                            >H.<surname>Vincent-Darasse</surname></persName> <lb/><metamark
                        function="separating" rend="horizontal-line"/> <lb/><metamark
                        function="other" rend="other"/></signed>
            </div>
            <div type="envelope">
                <pb facs="#FRAN_Poilus_t-0214_02"/>
                <p rend="left">Ceci est mon testament.</p>
                <metamark function="other" rend="horizontal-line"/>
            </div>
        </body>
    </text>
