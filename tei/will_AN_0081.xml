    <text xml:id="will_AN_0081">
        <body>
            <div type="will">
                <pb facs="#FRAN_Poilus_t-0081_01"/>
                <dateline rend="right"><placeName type="willPlace" ref="#pl-241"><choice><sic>Vaubécourt.</sic><corr>Vaubecourt</corr></choice></placeName>
                    <date type="willDate" when="1915-01-24">le 24 <choice><sic>Janvier</sic><corr>janvier</corr></choice>
                        <lb/>1915</date></dateline>
                <space quantity="2" unit="lines" dim="vertical"/>
                <p rend="left">Je lègue à ma <lb/>mère, <persName><choice><abbr>V<hi rend="superscript-underlined">ve</hi></abbr><expan>veuve</expan></choice> Moine</persName><choice><sic/><corr>,</corr></choice> <lb/>tout ce que je possède. <lb/>Fait sain de corps <lb/>et d’esprit.</p>
                <space quantity="1" unit="lines" dim="vertical"/>
                <dateline rend="right"><placeName type="willPlace" ref="#pl-241"><choice><sic>A Vaubécourt</sic><corr>À Vaubecourt</corr></choice> (Meuse)</placeName>
                    <lb/>le <date type="willDate" when="1915-01-24">24 <choice><sic>Janvier</sic><corr>janvier</corr></choice> 1915</date><choice><sic/><corr>.</corr></choice></dateline>
                <space quantity="1" unit="lines" dim="vertical"/>
                <signed rend="centered"><persName ref="#pers-138">Claudius <surname>Moine</surname></persName></signed>
            </div>
        </body>
    </text>
