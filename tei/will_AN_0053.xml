    <text xml:id="will_AN_0053">
        <body>
            <div type="will">
                <pb facs="#FRAN_Poilus_t-0053_01"/>
                <dateline><placeName type="willPlace" ref="#pl-127">Gentilly</placeName> 
    le <date type="willDate" when="1914-08-03">3 <choice><sic>aout</sic><corr>août</corr></choice> 1914</date></dateline>
                <space dim="vertical" unit="lines" quantity="5"/>
                <head>Ceci est mon testament</head>
                <p>Je <choice><sic>légue a</sic><corr>lègue à</corr></choice> <persName>Marguerite <lb/>Lelong</persName> <choice><sic>Ma</sic><corr>ma</corr></choice> femme tous les <lb/>biens qui <choice><sic>mapartiendront</sic><corr>m’appartiendont</corr></choice> <lb/><choice><sic>a</sic><corr>à</corr></choice> mon <choice><sic>décé</sic><corr>décès.</corr></choice> Je l’institue <lb/>ma légataire universelle<choice><sic/><corr>.</corr></choice></p>
                <dateline><choice><sic>fait a</sic><corr>Fait à</corr></choice>
                    <placeName type="willPlace" ref="#pl-127">Gentilly</placeName> le <date type="willDate" when="1914-08-03">3 <choice><sic>aout</sic><corr>août</corr></choice> 1914</date><choice><sic/><corr>.</corr></choice></dateline>
                <space quantity="1" unit="lines" dim="vertical"/>
                <signed rend="centered"><persName ref="#pers-110"><surname>Bailly</surname> Joseph</persName></signed>
            </div>
        </body>
    </text>
