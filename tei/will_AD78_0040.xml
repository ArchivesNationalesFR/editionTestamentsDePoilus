    <text xml:id="will_AD78_0040">
        <body>
            <div type="envelope">
                <pb facs="#FRAD78_Poilus_t-0040_01"/>
                <p rend="centered"><choice><sic>A</sic><corr>À</corr></choice>
                    <persName>Madame
                            <surname>Fagnou</surname></persName><choice><sic/><corr>.</corr></choice>
                    <lb/><choice><sic>mon</sic><corr>Mon</corr></choice>
                    <choice><sic>testamant</sic><corr>testament.</corr></choice></p>
                    <p><seg type="signature"><persName ref="#pers-38"><surname>Fagnou</surname>
                            Médéric</persName></seg></p>
                    <p><address>
                        <addrLine>90 rue Remilly <placeName ref="#pl-35">Versailles</placeName>
                            <choice><abbr>S O</abbr><expan>Seine-et-Oise</expan></choice></addrLine>
                    </address></p>
            </div>
            <div type="will">
                <pb facs="#FRAD78_Poilus_t-0040_03"/>
                <head><choice><sic>Testamant</sic><corr>Testament.</corr></choice>
                    <lb/>Ceci est mon
                    <choice><sic>testamant</sic><corr>testament.</corr></choice></head>
                <p>Je soussigné <persName ref="#pers-38">Médéric
                        <surname>Fagnou</surname></persName><lb/><choice><sic>
                            Demeurant</sic><corr>, demeurant</corr></choice> 30 rue Rémilly
                            <lb/><choice><sic>a</sic><corr>à</corr></choice>
                    <placeName type="willAuthorResidence" ref="#pl-35">Versailles</placeName><choice><sic> saint</sic><corr>,
                        sain</corr></choice> de corps <lb/>et d’esprit
                            <choice><sic>Nome</sic><corr>nomme</corr></choice> légataire <lb/>
                    universelle ma femme née <lb/><persName>Ernestine
                                <choice><sic>augustine</sic><corr>Augustine</corr></choice> Eugénie
                        Fauconnier <lb/></persName><choice><sic>tous ce que je possede présent <lb/>
                            et avenir</sic><corr>de tout ce que je possède, à présent et à
                            l’avenir.</corr></choice> <choice><sic>j’ai le
                            Double</sic><corr>J’ai mis le double</corr></choice>
                    <lb/>entre les mains de ma femme<choice><sic/><corr>.</corr></choice></p>
                <space quantity="2" unit="lines" dim="vertical"/>
                <dateline rend="centered"><placeName type="willPlace" ref="#pl-35">Versailles</placeName>
                    <date type="willDate" when="1914-08-03">le trois
                                <choice><sic>Aout</sic><corr>août</corr></choice>
                        <lb/><space quantity="1" unit="lines" dim="vertical"/>mille neuf cent
                                quatorze<choice><sic/><corr>.</corr></choice></date></dateline>
                <space quantity="2" unit="lines" dim="vertical"/>
                <signed rend="left">
                    <persName ref="#pers-38">Médéric <surname>Fagnou</surname></persName>
                </signed>
            </div>
        </body>
    </text>
