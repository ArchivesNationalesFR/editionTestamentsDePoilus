    <text xml:id="will_AN_0186">
        <body>
            <div type="will">
                <pb facs="#FRAN_Poilus_t-0186_01"/>
                <dateline><placeName type="willPlace" ref="#pl-2">Paris</placeName>
                    <date type="willDate" when="1914-08-03">le trois août 1914</date></dateline>
                <space dim="vertical" unit="lines" quantity="7"/>
                <p>Je soussigné <persName ref="#pers-248"><surname>Delquigny</surname>
                        <lb/>Marius</persName> institue par le présent <lb/>testament<choice><sic>
                            ;</sic><corr></corr></choice> ma femme <persName>Madame <lb/>Delquigny
                        née <surname>Dewez</surname> Eva
                        <subst><del>E</del><add>H</add></subst>éléna</persName>
                    <lb/><choice><sic>mon légataire universel</sic><corr>ma légataire
                            universelle.</corr></choice></p>
                <dateline>Fait à <placeName type="willPlace" ref="#pl-2">Paris</placeName>
                    <date type="willDate" when="1914-08-03">le trois août <lb/>mil neuf cent
                        quatorze</date><choice><sic/><corr>.</corr></choice></dateline>
                <space dim="vertical" unit="lines" quantity="2"/>
                <signed rend="centered"><persName ref="#pers-248">M.
                        <surname>Delquigny</surname></persName></signed>
            </div>
        </body>
    </text>
