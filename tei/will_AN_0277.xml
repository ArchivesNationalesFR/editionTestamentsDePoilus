    <text xml:id="will_AN_0277">
        <body>
            <div type="will">
                <pb facs="#FRAN_Poilus_t-0277_01"/>
                <dateline rend="centered"><placeName type="willPlace" ref="#pl-253"
                        >Limoges</placeName> <date type="willDate" when="1914-10-21">le 21
                                    <choice><abbr>Oct<hi rend="superscript-underlined"
                                >bre</hi></abbr><expan>octobre</expan></choice>
                            1914</date><choice><sic/><corr>.</corr></choice></dateline>
                <space dim="vertical" unit="lines" quantity="1"/>
                <head><choice><sic>Ce ci</sic><corr>Ceci</corr></choice> est mon
                            testament<choice><sic/><corr>.</corr></choice></head>
                <p>Je donne à <persName>Madame
                        Robert <lb/>née Maria Delmas</persName> la somme de
                            <lb/><choice><sic>onze-cent</sic><corr>onze-cents</corr></choice> francs
                            <choice><sic>a</sic><corr>à</corr></choice> prendre sur mon <lb/>livret
                    après ma mort.</p>
                <space dim="vertical" unit="lines" quantity="1"/>
                <p>Numéro de mon livret <choice><sic/><corr>:</corr></choice> 75-2.275.367.</p>
                <space dim="vertical" unit="lines" quantity="1"/>
                <p><persName>Madame Robert</persName> étant porteur <lb/>de mon livret.</p>
                <space dim="vertical" unit="lines" quantity="1"/>
                <dateline rend="centered"><placeName type="willPlace" ref="#pl-253"
                        >Limoges</placeName> <date type="willDate" when="1914-10-21">le 21
                                <choice><sic>Octobre</sic><corr>octobre</corr></choice>
                            1914</date><choice><sic/><corr>.</corr></choice></dateline>
                <space dim="vertical" unit="lines" quantity="2"/>
                <signed rend="centered"><persName ref="#pers-331"
                    >Henri.Alexandre.Raud</persName></signed>
            </div>
        </body>
    </text>
