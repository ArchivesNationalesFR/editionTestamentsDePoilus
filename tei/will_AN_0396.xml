    <text xml:id="will_AN_0396">
        <body>
            <div type="will">
                <pb facs="#FRAN_Poilus_t-0396_01"/>
                <dateline rend="centered"><placeName type="willPlace" ref="#pl-2">Paris</placeName> <date type="willDate" when="1914-08-12">le 12
                                <choice><sic>Aout</sic><corr>août</corr></choice>
                    1914</date></dateline>
                <space dim="vertical" unit="lines" quantity="4"/>
                <p>Je <choice><sic>soussigne</sic><corr>soussigné</corr></choice> <persName>Monsieur
                            <lb/><surname>Grosbois</surname>
                            Gustave</persName><choice><sic/><corr>,</corr></choice> <lb/><choice><sic>J autorise a ma
                            Mére</sic><corr>j’autorise ma mère</corr></choice> <lb/><persName>Madame
                                <choice><sic>Veuve</sic><corr>veuve</corr></choice>
                        Grosbois</persName><choice><sic/><corr>,</corr></choice> <lb/><choice><sic>Marier
                            en deuzieme</sic><corr>mariée en
                            deuxièmes</corr></choice> <lb/><choice><sic>noce</sic><corr>noces</corr></choice>
                    avec <persName><choice><abbr>M<hi rend="superscript">r</hi></abbr><expan>Monsieur</expan></choice> Chappel
                        <lb/>Victor</persName><choice><sic/><corr>,</corr></choice> <choice><sic>a
                            Toucher</sic><corr>à toucher</corr></choice> les <lb/>sommes et bijoux
                    et <choice><sic>bien</sic><corr>biens</corr></choice> <lb/>qui
                            <choice><sic>apartienne a</sic><corr>m’appartiennent en</corr></choice>
                    mon <lb/>lieu et place<choice><sic/><corr>.</corr></choice></p>
                <signed rend="right"><persName ref="#pers-538"><surname>Grosbois</surname>
                        Gustave</persName></signed>
                <space dim="vertical" unit="lines" quantity="1"/>
                <postscript>
                    <p><choice><sic>Cecy</sic><corr>Ceci</corr></choice> est mon
                                <choice><sic>Testament</sic><corr>testament.</corr></choice> <lb/>Je
                                <choice><sic>laygue a</sic><corr>lègue à</corr></choice> ma
                                <lb/><choice><sic>Mére tous se</sic><corr>mère tout
                            ce</corr></choice> que <lb/><choice><sic>Je posséde</sic><corr>je
                                possède.</corr></choice></p>
                </postscript>
                <space dim="vertical" unit="lines" quantity="1"/>
                <dateline><choice><sic>fait a</sic><corr>Fait à</corr></choice> <placeName type="willPlace" ref="#pl-2">Paris</placeName> <date type="willDate" when="1914-08-12">le 12
                        <choice><sic>Aout</sic><corr>août</corr></choice> <lb/>1914</date><choice><sic/><corr>.</corr></choice></dateline>
                <space dim="vertical" unit="lines" quantity="1"/>
                <signed rend="right"><persName ref="#pers-538"><surname>Grosbois</surname>
                        Gustave</persName></signed>
            </div>
        </body>
    </text>
