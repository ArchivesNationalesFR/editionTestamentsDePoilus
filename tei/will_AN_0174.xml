    <text xml:id="will_AN_0174">
        <body>
            <div type="will">
                <pb facs="#FRAN_Poilus_t-0174_01"/>
                <dateline rend="right"><placeName type="willPlace" ref="#pl-18">Verdun</placeName>
                    <date type="willDate" when="1914-11-13">le 13 novembre 1914</date></dateline>
                <space dim="vertical" unit="lines" quantity="3"/>
                <head rend="right">Ceci<choice><sic>,</sic><corr/></choice> est <choice><sic>Mon
                            Testament</sic><corr>mon testament.</corr></choice></head>
                <space dim="vertical" unit="lines" quantity="2"/>
                <p>Je, <choice><sic>Soussignée</sic><corr>soussigné</corr></choice> <persName
                        ref="#pers-236">Paul<choice><sic>.</sic><corr/></choice>
                                Henri<choice><sic>.</sic><corr/></choice>
                                    Antoine <supplied>Rogé</supplied></persName> <lb/>actuellement au <orgName
                        ref="#o-195">44<hi rend="superscript-double-underlined"
                                    >eme</hi> <choice><abbr>territorial</abbr><expan>régiment
                                d’infanterie
                            territoriale</expan></choice></orgName>&#xa0;<choice><sic>a</sic><corr>à</corr></choice> <lb/><placeName
                        type="militaryEventPlace" ref="#pl-18">Verdun</placeName> 7<hi
                                rend="superscript-double-underlined"
                            >eme</hi> <choice><sic>Compagnie.
                            Institue</sic><corr>compagnie, institue</corr></choice> <lb/>ma femme,
                    née <persName>Marie Trombert</persName><choice><sic></sic><corr>,</corr></choice> <lb/><choice><sic>Ma
                            Legataire</sic><corr>ma légataire</corr></choice> universelle.</p>
                <space dim="vertical" unit="lines" quantity="2"/>
                <dateline rend="right">Fait <choice><sic>a</sic><corr>à</corr></choice> <placeName
                        type="willPlace" ref="#pl-18">Verdun</placeName> <date type="willDate"
                        when="1914-11-13">le 13 novembre
                        1914<choice><sic/><corr>.</corr></choice></date></dateline>
                <space dim="vertical" unit="lines" quantity="1"/>
                <signed rend="centered"><persName ref="#pers-236">Paul
                        <surname>Rogé</surname></persName></signed>
            </div>
        </body>
    </text>
