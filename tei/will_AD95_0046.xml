    <text xml:id="will_AD95_0046">
        <body>
            <div type="will">
                <pb facs="#FRAD95_Poilus_t-0046_01"/>
                <dateline><date type="willDate" when="1917-02-08">Le 8
                                <choice><sic>Fevrier</sic><corr>février</corr></choice>
                    1917</date></dateline>
                <space dim="vertical" unit="lines" quantity="1"/>
                <p rend="left"><choice><sic>cher pere</sic><corr>Cher père, </corr></choice>
                          <lb/><choice><sic>Je</sic><corr>je</corr></choice> vous
                            <choice><sic>mest</sic><corr>mets</corr></choice> ce billet dans
                    <lb/>votre lettre que vous
                            <choice><sic>conserverais</sic><corr>conserverez.</corr></choice> <lb/><choice><sic>si</sic><corr>Si</corr></choice>
                    des fois je ne revenais
                            pas<choice><sic>,</sic><corr/></choice> <lb/><choice><sic>Je</sic><corr>je</corr></choice>
                    donne <choice><sic>ames</sic><corr>à mes</corr></choice> deux
                            <choice><sic>frere</sic><corr>frères</corr></choice> <lb/>Ambroise et
                    Joseph
                    <lb/><choice><sic>Lai Batise</sic><corr>les bâtisses</corr></choice> que
                    je <choice><sic>Posède</sic><corr>possède</corr></choice> <lb/>au
                    <choice><abbr>N</abbr><expan>numéro</expan></choice> 7 du
                    <choice><sic>Passage</sic><corr>passage</corr></choice> <lb/>Levée
                            <choice><sic>a</sic><corr>à</corr></choice> <placeName type="otherPlace" ref="#pl-2">Paris</placeName><choice><sic/><corr>.</corr></choice></p>
                <signed rend="centered"><persName ref="#pers-440">Ernest
                        <surname>Nantet</surname></persName> <lb/><orgName><choice><abbr>a L. G.
                                P.</abbr><expan>artillerie lourde à longue
                            puissance</expan></choice> 364</orgName> <lb/>Convoi Automobiles <lb/>Par
                                <orgName><choice><abbr>B. C. M.</abbr><expan>Bureaux centraux
                                militaires</expan></choice></orgName> <placeName type="otherPlace" ref="#pl-2">Paris</placeName><choice><sic/><corr>.</corr></choice></signed>
            </div>
        </body>
    </text>
