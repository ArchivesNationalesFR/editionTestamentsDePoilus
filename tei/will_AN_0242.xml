    <text xml:id="will_AN_0242">
        <body>
            <div type="will">
                <pb facs="#FRAN_Poilus_t-0242_01"/>
                <dateline rend="right"><placeName type="willPlace" ref="#pl-315"
                        >Bordeaux</placeName> <date type="willDate" when="1914-08-12">12
                            <choice><sic>Août</sic><corr>août</corr></choice>
                                <choice><abbr>14</abbr><expan>1914</expan></choice></date></dateline>
                <space dim="vertical" unit="lines" quantity="2"/>
                <head rend="centered"><choice><sic>Dernieres Volontees</sic><corr>Dernières
                            volontés</corr></choice></head>
                <space dim="vertical" unit="lines" quantity="2"/>
                <p>Je <choice><sic>soussigne</sic><corr>soussigné</corr></choice> <persName
                        ref="#pers-300">Jean
                            <surname>Dusserm</surname></persName> <lb/><choice><sic>reconnait</sic><corr>reconnais</corr></choice>
                    que tout ce que <lb/>je possède appartient
                        <choice><sic>a</sic><corr>à</corr></choice> ma <lb/>femme <persName>Jeanne
                        Dusserm</persName> <lb/><choice><sic><surplus>mon épouse</surplus></sic><corr></corr></choice> ; en quoi je
                            <lb/><choice><sic>fait</sic><corr>fais</corr></choice> cette lettre
                            <choice><sic>en</sic><corr>au</corr></choice> cas
                            <lb/><choice><sic>ou</sic><corr>où</corr></choice> je
                            <choice><sic>viendrait a desparaitre</sic><corr>viendrais à
                            disparaître</corr></choice> <lb/>et je ne
                            <choice><sic>connaît</sic><corr>connais</corr></choice> pas le <lb/>droit de lui demander des <lb/>comptes
                            <choice><sic>vis-a-vis</sic><corr>vis-à-vis</corr></choice> de ma
                    <lb/>famille en <choice><sic>quoi-sa-soit :</sic><corr>quoi que ce
                        soit.</corr></choice></p>
                <space dim="vertical" unit="lines" quantity="1"/>
                <dateline rend="right">Fait <choice><sic>a</sic><corr>à</corr></choice> <placeName
                        type="willPlace" ref="#pl-315">Bordeaux</placeName> <date type="willDate"
                            when="1914-08-12">le 12 <choice><sic>Août</sic><corr>août</corr></choice>
                        <lb/>1914</date><choice><sic></sic><corr>.</corr></choice></dateline>
                <signed rend="centered"><persName ref="#pers-300">Jean
                        <surname>Dusserm</surname></persName></signed>
            </div>
            <div type="envelope">
                <pb facs="#FRAN_Poilus_t-0242_02ab"/>
                <p><persName>Madame Dusserm
                        Jeanne</persName><choice><sic></sic><corr>,</corr></choice> <lb/><choice><sic>a</sic><corr>à</corr></choice> ouvrir
                            <choice><sic>apres</sic><corr>après</corr></choice>&#xa0;<choice><sic>Dècès</sic><corr>décès</corr></choice>.</p>
            </div>
        </body>
    </text>
