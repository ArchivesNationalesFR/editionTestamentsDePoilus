    <text xml:id="will_AN_0084">
        <body>
            <div type="will">
                <pb facs="#FRAN_Poilus_t-0084_01"/>
                <head>Ceci est mon testament</head>
                <dateline><choice><sic>fait</sic><corr>Fait</corr></choice> ce jour<choice><sic/><corr>,</corr></choice> <date when="1914-08-04" type="willDate">quatre
                                <lb/><choice><sic>Août</sic><corr>août</corr></choice> mille neuf
                        cent <lb/>quatorze.</date></dateline>
                <p>Je fais ma femme <lb/><persName>Anne Marie Bellanger</persName> ma <lb/>légataire
                            universelle<choice><sic/><corr>.</corr></choice> <choice><sic>par</sic><corr>Par</corr></choice>
                    ce <lb/>présent je reconnais que c’est <lb/>elle qui m’a
                            <choice><sic>apporter</sic><corr>apporté</corr></choice> en <lb/>mariage
                        <add place="above">une somme</add> de trois mille
                            francs<choice><sic/><corr>.</corr></choice></p>
                <space dim="vertical" unit="lines" quantity="1"/>
                <dateline><placeName type="willPlace" ref="#pl-2">Paris</placeName> <date when="1914-08-04" type="willDate">le 4
                            <choice><sic>Aout</sic><corr>août</corr></choice>
                    1914</date><choice><sic/><corr>,</corr></choice> <lb/>9
                        Bou<subst><del>l</del><add>le</add></subst>vard Arago<choice><sic><surplus>9</surplus></sic><corr>.</corr></choice></dateline>
                <signed rend="right"><persName ref="#pers-140"><surname>Le Pocréau</surname>
                        Ambroise</persName></signed>
            </div>
        </body>
    </text>
