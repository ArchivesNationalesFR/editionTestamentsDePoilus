    <text xml:id="will_AN_0369">
        <body>
            <div type="will">
                <pb facs="#FRAN_Poilus_t-0369_01"/>
                <dateline><placeName type="willPlace" ref="#pl-2">Paris</placeName> le <date type="willDate" when="1916-01-03">trois
                            <choice><sic>J</sic><corr>j</corr></choice>anvier mille neuf
                                <choice><sic>cents sen<add place="above">i</add>ze</sic><corr>cent
                                seize</corr></choice></date></dateline>
                <space dim="vertical" unit="lines" quantity="3"/>
                <head rend="centered">Ceci est mon
                    testament</head>
                <space dim="vertical" unit="lines" quantity="3"/>
                <p>J’institue ma mère <persName>Jeanne Pinot <lb/>née Jeanne Thuillier</persName> ma
                    légataire <lb/>universelle et lui laisse tout ce que <lb/>la loi m’autorise.</p>
                <space dim="vertical" unit="lines" quantity="1"/>
                <dateline><placeName type="willPlace" ref="#pl-2">Paris</placeName> le <date type="willDate" when="1916-01-03"><subst><del>3</del><add>t</add></subst>rois <choice><sic>J</sic><corr>j</corr></choice>anvier mille neuf
                                <choice><sic>cents</sic><corr>cent</corr></choice>
                            seize</date></dateline>
                <space dim="vertical" unit="lines" quantity="2"/>
                <signed rend="centered"><persName ref="#pers-510">H <surname>Pinot</surname></persName></signed>
            </div>
            <div type="envelope">
                <pb facs="#FRAN_Poilus_t-0369_02"/>
                <p rend="centered">Ceci est mon testament<choice><sic/><corr>.</corr></choice></p>
                <space dim="vertical" unit="lines" quantity="2"/>
                <signed rend="centered"><persName ref="#pers-510">H <surname>Pinot</surname></persName></signed>
            </div>
        </body>
